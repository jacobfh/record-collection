<h1>New Album</h1>


<%= form_with scope: :album, url: albums_path, local: true do |form| %>
    <p>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
    </p>

    <p>
    <%= form.label :artist %><br>
    <%= form.text_field :artist %>
    </p>

    <p>
      <%= form.label :released %><br>
      <%= form.text_field :released %>
    </p>
   
    <p>
      <%= form.submit %>
    </p>
  <% end %>